{
  "name": "forsyth-2019",
  "version": "1.0.0",
  "author": "Laura Brown",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist/*",
    "dev": "npm run clean && webpack --colors --mode development && theme deploy && npm run watch",
    "watch": "concurrently --prefix name --names 'webpack,theme watch' --prefix-colors yellow,blue \"webpack --watch --colors --mode development\" \"theme watch\"",
    "start": "npm run dev",
    "download:settings": "theme download config/settings_data.json --dir=src/ --no-ignore",
    "build": "npm run clean && webpack --mode production",
    "deploy": "npm run build && theme deploy"
  },
  "dependencies": {
    "@shopify/theme-a11y": "^3.1.0",
    "@shopify/theme-addresses": "1.0",
    "@shopify/theme-cart": "^3.1.0",
    "@shopify/theme-currency": "^3.1.0",
    "@shopify/theme-images": "^3.1.0",
    "@shopify/theme-product": "^3.1.0",
    "@shopify/theme-product-form": "^3.1.0",
    "@shopify/theme-sections": "^3.1.0",
    "breakpoints-js": "^1.0.6",
    "core-js": "^3.15.1",
    "eslint": "^4.19.1",
    "flickity": "^2.2.2",
    "flickity-fade": "^1.0.0",
    "flickity-imagesloaded": "^2.0.0",
    "imagesloaded": "^4.1.4",
    "infinite-scroll": "^3.0.6",
    "jquery": "^3.6.0",
    "lazysizes": "^5.3.2",
    "lodash-es": "^4.17.21",
    "masonry-layout": "^4.2.2",
    "mela": "^1.3.3",
    "qrcode": "^1.4.4",
    "quicklink": "^2.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "regenerator-runtime": "^0.13.7",
    "sass-lint-webpack": "^1.0.3",
    "sticky-sidebar": "^3.3.1",
    "svbstrate": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@shopify/themekit": "^1.1.9",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "concurrently": "5.2",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^4.3.0",
    "cssnano-webpack-plugin": "^1.0.3",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "postcss-discard-comments": "^4.0.0",
    "sass-loader": "^9.0.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  }
}
