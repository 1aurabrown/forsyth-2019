{
  "name": "peoples-2020",
  "version": "1.0.0",
  "author": "Laura Brown",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist/*",
    "dev": "yarn clean && webpack --colors --mode development && theme deploy && yarn watch",
    "watch": "concurrently --prefix name --names 'webpack,theme watch' --prefix-colors yellow,blue \"webpack --watch --colors --mode development\" \"theme watch\"",
    "start": "yarn dev",
    "download:settings": "theme download config/settings_data.json --dir=src/ --no-ignore",
    "build": "yarn clean && webpack --mode production",
    "deploy": "yarn build && theme deploy"
  },
  "dependencies": {
    "@shopify/theme-a11y": "^3.0.3",
    "@shopify/theme-addresses": "1.0",
    "@shopify/theme-cart": "^3.0.3",
    "@shopify/theme-currency": "^3.0.3",
    "@shopify/theme-images": "^3.1.0",
    "@shopify/theme-product": "^3.0.3",
    "@shopify/theme-product-form": "^3.0.3",
    "@shopify/theme-sections": "^3.0.3",
    "breakpoints-js": "^1.0.6",
    "core-js": "^3.6.4",
    "eslint": "^4.18.2",
    "flickity": "^2.2.1",
    "flickity-fade": "^1.0.0",
    "flickity-imagesloaded": "^2.0.0",
    "imagesloaded": "^4.1.4",
    "infinite-scroll": "^3.0.6",
    "jquery": "^3.5.1",
    "lazysizes": "^5.2.1",
    "lodash-es": "^4.17.15",
    "masonry-layout": "^4.2.2",
    "mela": "^1.2.1",
    "qrcode": "^1.4.4",
    "quicklink": "2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "regenerator-runtime": "^0.13.5",
    "sass-lint-webpack": "^1.0.3",
    "sticky-sidebar": "^3.3.1",
    "svbstrate": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.11",
    "@babel/preset-env": "7.11",
    "@shopify/themekit": "^1.1.4",
    "autoprefixer": "9.8",
    "babel-loader": "^8.1.0",
    "concurrently": "5.2",
    "copy-webpack-plugin": "6.0",
    "css-loader": "^4.1.1",
    "cssnano": "^4.1.10",
    "cssnano-webpack-plugin": "^1.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
